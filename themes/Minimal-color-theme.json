{
	"name": "dantos",
	"type": "dark",
	"colors": {
		// The theme itself
		"editor.background": "#282a36",
		"editor.foreground": "#f8f8f2",
		"activityBarBadge.background": "#555555",
		"sideBarTitle.foreground": "#e5e5e5",
		// Tabs
		"tab.activeForeground": "#fff",
		"tab.unfocusedActiveForeground": "#fff",
		"tab.inactiveForeground": "#9c9c9c",
		"tab.unfocusedInactiveForeground": "#9c9c9c",
		"tab.activeBackground": "#282a36",
		"tab.unfocusedActiveBorder": "#1d1f26",
		"tab.inactiveBackground": "#1d1f26",
		"tab.activeBorder": "#1d1f26",
		"tab.border": "#1d1f26",
		"editorGroupHeader.tabsBorder": "#1d1f26",
		// Title bar (requires "window.titleBarStyle": "custom" in settings)
		"titleBar.activeBackground": "#1d1f26",
		"titleBar.inactiveBackground": "#1d1f26",
		"titleBar.border": "#1d1f26",
		"titleBar.activeForeground": "#ccc",
		"titleBar.inactiveForeground": "#ccc",
		// Scrollbar
		"scrollbarSlider.background": "#fff2",
		"scrollbarSlider.hoverBackground": "#fff3",
		"scrollbarSlider.activeBackground": "#fff4",
		// Backgrounds
		"statusBar.background": "#1d1f26",
		"statusBar.noFolderBackground": "#1d1f26",
		"editorGroup.emptyBackground": "#1d1f26",
		"editorGroup.dropBackground": "#1d1f26",
		"editorGroupHeader.noTabsBackground": "#1d1f26",
		"editorGroupHeader.tabsBackground": "#1d1f26",
		"activityBar.background": "#1d1f26",
		// Misc
		"statusBar.foreground": "#ccc",
		"sideBar.background": "#1d1f26",
		"sideBar.border": "#222",
		"editorRuler.foreground": "#222",
		"editorGroup.border": "#1d1f26",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#6272a4"
			}
		},
		{
			"name": "String, Regex",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#f0f0ff"
			}
		}
	]
}