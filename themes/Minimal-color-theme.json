{
	"name": "hackerman",
	"type": "dark",
	"colors": {
		// The theme itself
		"editor.background": "#000",
		"editor.foreground": "#00ff00",
		"activityBarBadge.background": "#555555",
		"sideBarTitle.foreground": "#e5e5e5",
		// Tabs
		"tab.activeForeground": "#fff",
		"tab.unfocusedActiveForeground": "#fff",
		"tab.inactiveForeground": "#9c9c9c",
		"tab.unfocusedInactiveForeground": "#9c9c9c",
		"tab.activeBackground": "#000",
		"tab.unfocusedActiveBorder": "#111",
		"tab.inactiveBackground": "#272727",
		"tab.activeBorder": "#111",
		"tab.border": "#111",
		"editorGroupHeader.tabsBorder": "#111",
		// Title bar (requires "window.titleBarStyle": "custom" in settings)
		"titleBar.activeBackground": "#111",
		"titleBar.inactiveBackground": "#111",
		"titleBar.border": "#111",
		"titleBar.activeForeground": "#ccc",
		"titleBar.inactiveForeground": "#ccc",
		// Scrollbar
		"scrollbarSlider.background": "#9992",
		"scrollbarSlider.hoverBackground": "#9993",
		"scrollbarSlider.activeBackground": "#9994",
		// Backgrounds
		"statusBar.background": "#111",
		"statusBar.noFolderBackground": "#111",
		"editorGroup.emptyBackground": "#111",
		"editorGroup.dropBackground": "#111",
		"editorGroupHeader.noTabsBackground": "#111",
		"editorGroupHeader.tabsBackground": "#111",
		"activityBar.background": "#111",
		// Misc
		"statusBar.foreground": "#ccc",
		"sideBar.background": "#000001",
		"sideBar.border": "#222",
		"editorRuler.foreground": "#222",
		"editorGroup.border": "#111",
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#006900"
			}
		},
		{
			"name": "String, Regex",
			"scope": [
				"string"
			],
			"settings": {
				"foreground": "#00ff69"
			}
		}
	]
}